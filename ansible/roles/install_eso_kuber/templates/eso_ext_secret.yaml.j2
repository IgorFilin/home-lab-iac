apiVersion: external-secrets.io/v1
kind: ExternalSecret
metadata:
  name: my-external-secret
  namespace: default
spec:
  refreshInterval: "2m"
  secretStoreRef:
    name: external-secret-store
    kind: ClusterSecretStore
  target:
    name: k3s-external-secret 
    creationPolicy: Owner
  dataFrom:
    - extract:
        key: external-secrets